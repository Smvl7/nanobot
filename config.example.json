{
  "agents": {
    "defaults": {
      "workspace": "~/.nanobot/workspace",
      "model": "anthropic/claude-opus-4-5",
      "timezone": "UTC",
      "max_tokens": 8192,
      "temperature": 0.7,
      "max_tool_iterations": 20
    }
  },
  "channels": {
    "whatsapp": {
      "enabled": false,
      "bridge_url": "ws://localhost:3001",
      "allow_from": []
    },
    "telegram": {
      "enabled": false,
      "token": "YOUR_TELEGRAM_BOT_TOKEN",
      "allow_from": [],
      "proxy": null
    },
    "discord": {
      "enabled": false,
      "token": "YOUR_DISCORD_BOT_TOKEN",
      "allow_from": [],
      "gateway_url": "wss://gateway.discord.gg/?v=10&encoding=json",
      "intents": 37377
    },
    "feishu": {
      "enabled": false,
      "app_id": "YOUR_FEISHU_APP_ID",
      "app_secret": "YOUR_FEISHU_APP_SECRET",
      "encrypt_key": "",
      "verification_token": "",
      "allow_from": []
    },
    "dingtalk": {
      "enabled": false,
      "client_id": "YOUR_DINGTALK_CLIENT_ID",
      "client_secret": "YOUR_DINGTALK_CLIENT_SECRET",
      "allow_from": []
    }
  },
  "providers": {
    "anthropic": {
      "api_key": "YOUR_ANTHROPIC_API_KEY",
      "api_base": null,
      "extra_headers": null
    },
    "openai": {
      "api_key": "YOUR_OPENAI_API_KEY",
      "api_base": null,
      "extra_headers": null
    },
    "openrouter": {
      "api_key": "YOUR_OPENROUTER_API_KEY",
      "api_base": null,
      "extra_headers": null
    },
    "deepseek": {
      "api_key": "YOUR_DEEPSEEK_API_KEY",
      "api_base": null,
      "extra_headers": null
    },
    "groq": {
      "api_key": "YOUR_GROQ_API_KEY",
      "api_base": null,
      "extra_headers": null
    },
    "zhipu": {
      "api_key": "YOUR_ZHIPU_API_KEY",
      "api_base": null,
      "extra_headers": null
    },
    "dashscope": {
      "api_key": "YOUR_DASHSCOPE_API_KEY",
      "api_base": null,
      "extra_headers": null
    },
    "vllm": {
      "api_key": "YOUR_VLLM_API_KEY",
      "api_base": "http://localhost:8000/v1",
      "extra_headers": null
    },
    "gemini": {
      "api_key": "YOUR_GEMINI_API_KEY",
      "api_base": null,
      "extra_headers": null
    },
    "moonshot": {
      "api_key": "YOUR_MOONSHOT_API_KEY",
      "api_base": null,
      "extra_headers": null
    },
    "aihubmix": {
      "api_key": "YOUR_AIHUBMIX_API_KEY",
      "api_base": null,
      "extra_headers": null
    }
  },
  "gateway": {
    "host": "0.0.0.0",
    "port": 18790
  },
  "tools": {
    "web": {
      "search": {
        "api_key": "YOUR_BRAVE_SEARCH_API_KEY",
        "max_results": 5
      }
    },
    "exec": {
      "timeout": 60
    },
    "restrict_to_workspace": false,
    "allow_file_ops": true,
    "allow_shell": false
  },
  "stt": {
    "provider": "groq",
    "api_key": "YOUR_GROQ_API_KEY"
  },
  "llm": {
    "provider": "google",
    "api_key": "YOUR_GOOGLE_API_KEY",
    "model": "gemini-2.0-flash-exp"
  }
}
